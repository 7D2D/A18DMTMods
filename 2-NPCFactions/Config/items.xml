<configs>

<!-- Relationship Values Chart 

	< 200 =  Hate
	< 400 =  Dislike
	< 600 =  Neutral	
	< 800 =  Like
	< 1001 = Love
	1001+ Leader	-->

	<append xpath="/items">
	
<!-- These faction schematics change your faction completely.  All other factions see you as this new faction  -->	
  
		<item name="MechFactionSchematic">
		  <property name="Extends" value="schematicMaster" />
		  <property name="DescriptionKey" value="MechFactionSchematicDesc" />
		  <property name="CreativeMode" value="Player" />
		  <property name="CustomIcon" value="schematicMaster" />
		  <effect_group tiered="false">
			<triggered_effect trigger="onSelfPrimaryActionEnd" action="ChangeFactionSDX, Mods" value="mechs" />
		  </effect_group>
		</item>

